<Application x:Class="MusicCloudPlayer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Controls/Colors.xaml"/>
                <ResourceDictionary Source="Controls/Geometry.xaml"/>
                <ResourceDictionary Source="Controls/Button.xaml"/>
                <ResourceDictionary Source="Controls/ToggleButton.xaml"/>
                <ResourceDictionary Source="Controls/Tab.xaml"/>
                <ResourceDictionary Source="Controls/TextBlock.xaml"/>
                <ResourceDictionary Source="Controls/ComboBox.xaml"/>
                <ResourceDictionary Source="Controls/ListBox.xaml"/>
                <ResourceDictionary Source="Controls/ListBoxStyle.xaml"/>
                <ResourceDictionary Source="Controls/Canvas.xaml"/>
                <ResourceDictionary Source="Controls/metroslider.xaml"/>
                <ResourceDictionary Source="Controls/progressbarslider.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
