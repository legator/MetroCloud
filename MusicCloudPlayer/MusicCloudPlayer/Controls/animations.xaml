<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Storyboard x:Key="ControlButtonPressAnim">
    <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="RenderTransform.Y">
      <DiscreteDoubleKeyFrame KeyTime="0:0:0.0" Value="0" />
      <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="-3">
        <EasingDoubleKeyFrame.EasingFunction>
          <ExponentialEase EasingMode="EaseOut" Exponent="6" />
        </EasingDoubleKeyFrame.EasingFunction>
      </EasingDoubleKeyFrame>
      <EasingDoubleKeyFrame KeyTime="0:0:0.6" Value="0">
        <EasingDoubleKeyFrame.EasingFunction>
          <ExponentialEase EasingMode="EaseOut" Exponent="6" />
        </EasingDoubleKeyFrame.EasingFunction>
      </EasingDoubleKeyFrame>
    </DoubleAnimationUsingKeyFrames>
  </Storyboard>
  <Storyboard x:Key="ControlButtonReleaseAnim">
    <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="RenderTransform.Y">
      <EasingDoubleKeyFrame KeyTime="0:0:0.6" Value="0">
        <EasingDoubleKeyFrame.EasingFunction>
          <ExponentialEase EasingMode="EaseOut" Exponent="6" />
        </EasingDoubleKeyFrame.EasingFunction>
      </EasingDoubleKeyFrame>
    </DoubleAnimationUsingKeyFrames>
  </Storyboard>
</ResourceDictionary>